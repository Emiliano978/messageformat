{
  "name": "messageformat-runtime",
  "version": "2.3.0",
  "description": "Runtime components of messageformat",
  "keywords": [
    "i18n",
    "icu",
    "messageformat",
    "internationalization"
  ],
  "author": "Eemeli Aro <eemeli@gmail.com>",
  "license": "MIT",
  "homepage": "https://messageformat.github.io/",
  "repository": "messageformat/messageformat",
  "main": "lib/messages.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "docs": "jsdoc2md -f src/messages.js > README.md",
    "prepublishOnly": "npm run build && npm run docs"
  },
  "babel": {
    "comments": false,
    "presets": [
      [
        "@babel/preset-env",
        {
          "exclude": [
            "transform-typeof-symbol"
          ],
          "loose": true
        }
      ]
    ]
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    }
  }
}
